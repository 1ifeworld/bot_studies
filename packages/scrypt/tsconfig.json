{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],    
    "moduleResolution": "node",
    // "target": "ESNext",
    // "module": "CommonJS",
    "module": "ESNext",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}

